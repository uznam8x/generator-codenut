<layout src="element/base.nunjucks">
  <block name="head">
    <link rel="stylesheet" href="/resource/stylesheet/layout/castle/style.css"/>
    <super />
  </block>
  <block name="content">
    <block name="header">
      <include src="./header.nunjucks" />
    </block>

    <main role="main" id="main">
      <aside class="basket">
        <block name="basket"></block>
      </aside>
      <aside class="bracket">
        <block name="bracket"></block>
      </aside>
      <section>
        <block name="main"></block>
      </section>
    </main>
    <block name="footer">
      <include src="./footer.nunjucks" />
    </block>
  </block>
</layout>
