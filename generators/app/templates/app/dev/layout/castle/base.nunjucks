{% extends "element/base.nunjucks" %}
{% block head %}
{{ super() }}
<link rel="stylesheet" href="/resource/stylesheet/layout/castle/style.css" />
{% endblock %}
{% block content %}
{% block header %} {% include './header.nunjucks' %} {% endblock %}

<main role="main" id="main">
  <aside class="basket">
    {% block basket %}{% endblock %}
  </aside>
  <aside class="bracket">
    {% block bracket %}{% endblock %}
  </aside>
  <section>
    {% block main %}{% endblock %}
  </section>
</main>
{% block footer %} {% include './footer.nunjucks' %} {% endblock %}
{% endblock %}
